type Mutation {
    createUser(input: CreateUserInput!) : User!
}

type Query {
    getUserById(id: String) : User
}

type User {
    firstName: String!
    lastName: String!
    email: String!
    phoneNumber: String!
    password: String!
    id: String!
}

input CreateUserInput {
    firstName: String!
    lastName: String!
    email: String!
    password: String!
    phoneNumber: String!
}
